# FOD
genfscon sysfs /sys/devices/platform/soc/4a88000.i2c/i2c-1/1-0038/touch/tp_dev/fod_status          u:object_r:sysfs_fod:s0
genfscon sysfs /sys/devices/platform/soc/soc:qcom,dsi-display-primary/fod_ui                       u:object_r:sysfs_fod:s0

# Touchpanel
genfscon sysfs /touchpanel                                      u:object_r:sysfs_touchpanel:s0
